<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uta.eventapp.uta_event">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".Login" />
        <activity android:name=".HomeScreen" />
        <activity android:name=".BroadcastEvent" />
        <activity android:name=".ViewEvent" />
        <activity android:name=".ViewDetailEvent" />
        <activity android:name=".Admin_Console" />
        <activity android:name=".AdminVerifyEvent" />
        <activity android:name=".AdminDeleteUser" />
        <activity android:name=".AdminDetailEvent" />
        <activity android:name=".Settings" />
        <activity android:name=".ResetPassword" />
        <activity android:name=".ModifyEvent" />
        <activity android:name=".ModifyEventDetail"></activity>
    </application>

</manifest>